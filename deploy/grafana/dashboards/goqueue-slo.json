        "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom", "showLegend": true },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "pluginVersion": "9.0.0",
      "targets": [
        {
          "expr": "(sum(rate(goqueue_broker_messages_failed_total[1h])) / sum(rate(goqueue_broker_messages_published_total[1h]))) / (1 - $slo_target/100)",
          "legendFormat": "1h Burn Rate",
          "refId": "A"
        },
        {
          "expr": "(sum(rate(goqueue_broker_messages_failed_total[6h])) / sum(rate(goqueue_broker_messages_published_total[6h]))) / (1 - $slo_target/100)",
          "legendFormat": "6h Burn Rate",
          "refId": "B"
        }
      ],
      "title": "Error Budget Burn Rate",
      "type": "timeseries"
    },
